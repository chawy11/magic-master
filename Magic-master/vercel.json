{
  "version": 2,
  "builds": [
    {
      "src": "Front/magic-trading-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/magic-trading-app"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Front/magic-trading-app/dist/magic-trading-app/index.html"
    }
  ]
}